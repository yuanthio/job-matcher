{
  "name": "job-matcher",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "cron:start": "ts-node --project tsconfig.cron.json src/services/cron-starter.ts",
    "cron:dev": "nodemon --exec ts-node --project tsconfig.cron.json src/services/cron-starter.ts",
    "telegram:bot": "node telegram-bot/bot.js",
    "telegram:dev": "nodemon telegram-bot/bot.js --ext js",
    "dev:all": "concurrently \"npm run dev\" \"npm run cron:dev\" \"npm run telegram:dev\""
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@lottiefiles/dotlottie-react": "^0.17.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/ssr": "^0.8.0",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cron": "^4.3.5",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.555.0",
    "next": "16.0.5",
    "next-themes": "^0.4.6",
    "node-telegram-bot-api": "^0.66.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/cron": "^2.0.1",
    "@types/node": "^20",
    "@types/node-telegram-bot-api": "^0.64.13",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.5",
    "nodemon": "^3.1.11",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}